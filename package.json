{
  "name": "aether-oracle",
  "version": "1.0.0",
  "description": "AI-powered oracle for AetherPay cross-border payment protocol",
  "main": "index.js",
  "scripts": {
    "start": "node oracle/server.js",
    "start:v2": "node oracle/server.js",
    "dev": "nodemon oracle/server.js",
    "collect": "python3 data/data_collector.py",
    "collect:historical": "python3 data/collect_historical.py",
    "train": "python3 models/train_model.py",
    "predict": "python3 aether_oracle.py predict",
    "status": "python3 aether_oracle.py status",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy-with-fxpool.js --network hardhat",
    "deploy:op-sepolia": "hardhat run scripts/deploy-with-fxpool.js --network op-sepolia",
    "deploy:oracle-v2": "hardhat run scripts/deploy-oracle-v2.js --network op-sepolia",
    "deploy:base-sepolia": "hardhat run scripts/deploy-with-fxpool.js --network base-sepolia",
    "deploy:optimism": "hardhat run scripts/deploy-with-fxpool.js --network optimism",
    "verify": "hardhat verify",
    "clean": "hardhat clean",
    "diagnose": "hardhat run scripts/diagnose-and-fix.js --network op-sepolia",
    "mint-tokens": "hardhat run scripts/mint-tokens.js --network op-sepolia",
    "deploy:mock-tokens": "hardhat run scripts/deploy-mock-tokens.js --network op-sepolia",
    "faucet": "hardhat run scripts/faucet.js --network op-sepolia",
    "deploy:new-mock-tokens": "hardhat run scripts/deploy-new-mock-tokens.js --network op-sepolia"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^7.2.0",
    "node-cron": "^3.0.2",
    "prom-client": "^15.1.3",
    "redis": "^4.6.8",
    "sqlite3": "^5.1.6",
    "winston": "^3.10.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^4.9.3",
    "chai": "^4.3.7",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.26.3",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1"
  },
  "resolutions": {
    "ethers": "^5.7.2"
  }
}
